<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<?import java.net.URL?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>
<fx:root resizable="false" title="Help" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
   <icons>
      <Image url="@/images/help_icon.png" />
   </icons>
   <scene>
      <Scene>
         <stylesheets>
            <URL value="@DarkTheme.css" />
            <URL value="@Extensions.css" />
         </stylesheets>
         <VBox alignment="CENTER_LEFT">
            <children>
               <Label fx:id="name" text="Label" styleClass="cell_big_label">
                  <VBox.margin>
                     <Insets right="5.0" />
                  </VBox.margin>
               </Label>
               <Label fx:id="customerId" text="Label">
                  <VBox.margin>
                     <Insets right="5.0" />
                  </VBox.margin>
               </Label>
               <Label fx:id="phone" text="Label">
                  <VBox.margin>
                     <Insets right="5.0" />
                  </VBox.margin>
               </Label>
               <Label fx:id="email" text="Label">
                  <VBox.margin>
                     <Insets right="5.0" />
                  </VBox.margin>
               </Label>
               <Label fx:id="address" text="Label">
                  <VBox.margin>
                     <Insets right="5.0" />
                  </VBox.margin>
               </Label>
               <VBox minWidth="580" prefWidth="580">
                  <WebView fx:id="map" minHeight="350" prefHeight="350"/>
               </VBox>
            </children>
            <opaqueInsets>
               <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
            </opaqueInsets>
            <padding>
               <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
            </padding>
         </VBox>
      </Scene>
   </scene>
</fx:root>
